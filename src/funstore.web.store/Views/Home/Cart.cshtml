@model List<CartItem>
@{
    ViewData["Title"] = "View Cart Contents";
}
<h2>@ViewData["Title"].</h2>
<h3>@ViewData["Message"]</h3>

<table>
    <thead><th>Item Id</th><th>Quantity</th></thead>
    <tbody>
        @if(Model?.Count > 0) {
            foreach(var i in Model){
                <tr><td>@i.ItemId</td><td>@i.Count</td></tr>
            }
        } else { <tr><td colspan="2">No items yet, go have some fun!</td></tr>}
    </tbody>
</table>
